{
  "name": "Crell/LiPHPe",
  "description": "An advanced version of Conway's Game of Life",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Larry Garfield",
      "email": "larry@garfieldtech.com"
    }
  ],
    "autoload": {
        "psr-4": {"Crell\\LiPHPe\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Crell\\LiPHPe\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^7.0",
        "doctrine/dbal": "^2.5",
        "ramsey/uuid": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit",
        "test-coverage": "php vendor/bin/phpunit --coverage-html coverage"
    }
}
